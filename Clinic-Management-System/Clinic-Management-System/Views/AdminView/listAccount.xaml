<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="Clinic_Management_System.Views.AdminView.listAccount"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Clinic_Management_System.Views.AdminView"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <ListView Name="itemsComboBox" 
          ItemsSource="{x:Bind ViewModel.Employees, Mode=OneWay}"
          ItemTemplate="{StaticResource HorizontalEmployee}"
          Width="400" Height="400">
        </ListView>
    </Grid>
</Page>
